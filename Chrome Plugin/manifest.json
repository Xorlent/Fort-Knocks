{
  "manifest_version": 3,
  "name": "Fort Knocks",
  "version": "0.1.3",
  "description": "Secure access pre-authentication tool",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
  
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/knocks16.png",
      "48": "icons/knocks48.png",
      "128": "icons/knocks128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/knocks16.png",
    "48": "icons/knocks48.png",
    "128": "icons/knocks128.png"
  }
} 